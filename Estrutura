-- --------------------------------------------------------

-- 
-- Estrutura da tabela `tab_users`
-- 

CREATE TABLE `tab_users` (
  `id` int(11) NOT NULL auto_increment,
  `primeira` tinyint(4) default NULL,
  `valida` tinyint(4) default NULL,
  `nome` varchar(200) default NULL,
  `sobrenome` varchar(200) default NULL,
  `login` varchar(200) default NULL,
  `senha` varchar(200) default NULL,
  `data_dia` int(2) default NULL,
  `data_mes` int(2) default NULL,
  `data_ano` int(4) default NULL,
  `sexo` tinyint(4) default NULL,
  `pais` varchar(200) default NULL,
  `estado` varchar(200) default NULL,
  `cidade` varchar(200) default NULL,
  `email` varchar(200) default NULL,
  `bloq` tinyint(4) default NULL,
  `plano` tinyint(4) default NULL,
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=6 ;

-- 
-- Extraindo dados da tabela `tab_users`
-- 


-- --------------------------------------------------------

-- 
-- Estrutura da tabela `tab_valida`
-- 

CREATE TABLE `tab_valida` (
  `cod` varchar(250) default NULL,
  `login` varchar(250) default NULL
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

