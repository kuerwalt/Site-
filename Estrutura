-- Estrutura da tabela `noticias_categorias`
-- 

CREATE TABLE `noticias_categorias` (
  `id` int(3) NOT NULL auto_increment,
  `nome` varchar(255) NOT NULL default '',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=10 ;

