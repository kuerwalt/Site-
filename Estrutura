-- 
-- Estrutura da tabela `phpsp_sessions`
-- 

CREATE TABLE `phpsp_sessions` (
  `id` char(20) NOT NULL default '',
  `LastAction` datetime NOT NULL default '0000-00-00 00:00:00',
  `ip` char(15) NOT NULL default '',
  `userID` mediumint(9) default NULL,
  PRIMARY KEY  (`id`),
  UNIQUE KEY `id_2` (`id`),
  KEY `id` (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

-- 
-- Extraindo dados da tabela `phpsp_sessions`
-- 


-- --------------------------------------------------------

-- 
-- Estrutura da tabela `phpsp_sessionvars`
-- 

CREATE TABLE `phpsp_sessionvars` (
  `id` mediumint(8) unsigned NOT NULL auto_increment,
  `session` varchar(20) NOT NULL default '',
  `name` varchar(32) NOT NULL default '',
  `intval` int(10) unsigned default NULL,
  `strval` varchar(100) default NULL,
  PRIMARY KEY  (`id`),
  UNIQUE KEY `id` (`id`),
  KEY `sessionID` (`session`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- 
-- Extraindo dados da tabela `phpsp_sessionvars`
-- 


-- --------------------------------------------------------

-- 
-- Estrutura da tabela `phpsp_users`
-- 

CREATE TABLE `phpsp_users` (
  `primary_key` mediumint(8) unsigned NOT NULL auto_increment,
  `id_franquia` char(3) collate latin1_general_ci NOT NULL default '',
  `nome` varchar(255) collate latin1_general_ci NOT NULL default '',
  `cpf` varchar(11) collate latin1_general_ci NOT NULL default '',
  `niver` date NOT NULL default '0000-00-00',
  `sexo` varchar(9) collate latin1_general_ci NOT NULL default '',
  `endereco` varchar(255) collate latin1_general_ci NOT NULL default '',
  `complemento` varchar(50) collate latin1_general_ci NOT NULL default '',
  `numero` varchar(5) collate latin1_general_ci NOT NULL default '',
  `bairro` varchar(200) collate latin1_general_ci NOT NULL default '',
  `cep` varchar(9) collate latin1_general_ci NOT NULL default '',
  `cidade` varchar(200) collate latin1_general_ci NOT NULL default '',
  `estado` char(2) collate latin1_general_ci NOT NULL default '',
  `ddd1` varchar(5) collate latin1_general_ci NOT NULL default '',
  `fone1` varchar(9) collate latin1_general_ci NOT NULL default '',
  `ddd2` varchar(5) collate latin1_general_ci NOT NULL default '',
  `fone2` varchar(9) collate latin1_general_ci NOT NULL default '',
  `email` varchar(255) collate latin1_general_ci NOT NULL default '',
  `msn` varchar(255) collate latin1_general_ci NOT NULL default '',
  `site` varchar(255) collate latin1_general_ci NOT NULL default '',
  `user` varchar(50) collate latin1_general_ci NOT NULL default '',
  `password` varchar(32) collate latin1_general_ci NOT NULL default '',
  `userlevel` tinyint(3) default NULL,
  PRIMARY KEY  (`primary_key`),
  KEY `user` (`user`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COLLATE=latin1_general_ci AUTO_INCREMENT=3 ;

