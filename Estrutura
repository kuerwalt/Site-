-- 
-- Estrutura da tabela `enquete_user`
-- 

CREATE TABLE `enquete_user` (
  `user_id` smallint(5) NOT NULL auto_increment,
  `username` varchar(30) NOT NULL default '',
  `userpass` varchar(32) NOT NULL default '',
  `session` varchar(32) NOT NULL default '',
  `last_visit` int(11) NOT NULL default '0',
  PRIMARY KEY  (`user_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- 
-- Extraindo dados da tabela `enquete_user`
-- 


-- --------------------------------------------------------

-- 
-- Estrutura da tabela `estat_users`
-- 

CREATE TABLE `estat_users` (
  `cod` tinyint(3) NOT NULL auto_increment,
  `login` varchar(20) NOT NULL default '',
  `senha` varchar(20) NOT NULL default '',
  `nome` varchar(50) NOT NULL default '',
  PRIMARY KEY  (`cod`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=2 ;

