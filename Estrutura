-- 
-- Estrutura da tabela `ondecomer`
-- 

CREATE TABLE `ondecomer` (
  `id` int(4) NOT NULL default '0',
  `idcid` varchar(4) NOT NULL default '',
  `nome` varchar(100) NOT NULL default '',
  `end` varchar(100) NOT NULL default '',
  `tel` varchar(100) NOT NULL default '',
  `especialidade` varchar(255) NOT NULL default '',
  `pastaf` varchar(100) NOT NULL default '',
  `foto1` varchar(100) NOT NULL default '',
  `foto2` varchar(100) NOT NULL default '',
  `foto3` varchar(100) NOT NULL default '',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

