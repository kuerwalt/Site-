-- 
-- Estrutura da tabela `ondeficar`
-- 

CREATE TABLE `ondeficar` (
  `id` int(4) NOT NULL default '0',
  `idcid` varchar(4) NOT NULL default '',
  `categoria` char(2) NOT NULL default '',
  `nome` varchar(100) NOT NULL default '',
  `endereco` varchar(150) NOT NULL default '',
  `opcoes` longtext NOT NULL,
  `tel` varchar(13) NOT NULL default '',
  `email` varchar(100) NOT NULL default '',
  `site` varchar(100) NOT NULL default '',
  `pastaf` varchar(100) NOT NULL default '',
  `foto1` varchar(40) NOT NULL default '',
  `foto2` varchar(40) NOT NULL default '',
  `foto3` varchar(40) NOT NULL default '',
  `foto4` varchar(40) NOT NULL default '',
  `foto5` varchar(40) NOT NULL default '',
  `foto6` varchar(40) NOT NULL default '',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

