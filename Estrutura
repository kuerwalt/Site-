CREATE TABLE `player` (
  `id` int(3) NOT NULL auto_increment,
  `id_cat` char(3) NOT NULL default '',
  `id_franquia` char(3) NOT NULL default '',
  `nome` varchar(255) NOT NULL default '',
  `data` date NOT NULL default '0000-00-00',
  `local` varchar(255) NOT NULL default '',
  `pasta` varchar(255) NOT NULL default '',
  `foto01` varchar(255) NOT NULL default '',
  `destaque` char(1) NOT NULL default 'S',
  `logo` enum('S','N') NOT NULL default 'S',
  `perfil` text NOT NULL,
  `por` varchar(255) NOT NULL default '',
  `fundo` varchar(255) NOT NULL default '',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

