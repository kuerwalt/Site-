-- --------------------------------------------------------

-- 
-- Estrutura da tabela `tab_coments`
-- 

CREATE TABLE `tab_coments` (
  `id` int(11) NOT NULL auto_increment,
  `id_user` int(11) default NULL,
  `id_post` int(11) default NULL,
  `nome` varchar(250) default NULL,
  `email` varchar(250) default NULL,
  `flog` varchar(250) default NULL,
  `comentario` longtext,
  `data` timestamp NOT NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP,
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- 
-- Extraindo dados da tabela `tab_coments`
-- 


-- --------------------------------------------------------

-- 
-- Estrutura da tabela `tab_denuncia`
-- 

CREATE TABLE `tab_denuncia` (
  `id` int(11) NOT NULL auto_increment,
  `nome` varchar(200) default NULL,
  `email` varchar(200) default NULL,
  `login_denunciado` varchar(200) default NULL,
  `url` varchar(200) default NULL,
  `data` timestamp NOT NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP,
  `descri` longtext,
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- 
-- Extraindo dados da tabela `tab_denuncia`
-- 


-- --------------------------------------------------------

-- 
-- Estrutura da tabela `tab_fotos`
-- 

CREATE TABLE `tab_fotos` (
  `id` int(11) NOT NULL auto_increment,
  `id_user` int(11) default NULL,
  `url_completa` varchar(250) default NULL,
  `url` varchar(250) default NULL,
  `titulo` varchar(250) default NULL,
  `comentario` longtext,
  `data` timestamp NOT NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP,
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- 
-- Extraindo dados da tabela `tab_fotos`
-- 


