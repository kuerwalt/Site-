CREATE TABLE `leis` (
  `id` int(9) NOT NULL auto_increment,
  `id_categoria` varchar(25) collate latin1_general_ci default NULL,
  `nome` varchar(255) collate latin1_general_ci NOT NULL default '',
  `descricao` text collate latin1_general_ci NOT NULL,
  `arquivo` varchar(255) collate latin1_general_ci NOT NULL,
  `visitas` varchar(25) collate latin1_general_ci default '0',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COLLATE=latin1_general_ci AUTO_INCREMENT=2 ;

