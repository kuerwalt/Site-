-- 
-- Estrutura da tabela `video`
-- 

CREATE TABLE `video` (
  `id` int(3) NOT NULL auto_increment,
  `id_franquia` char(3) NOT NULL default '',
  `id_galeria` varchar(5) NOT NULL default '',
  `categoria` varchar(50) NOT NULL default '',
  `foto` varchar(255) NOT NULL default '',
  `titulo` varchar(255) NOT NULL default '',
  `texto` varchar(255) NOT NULL default '',
  `previa` varchar(255) NOT NULL default '',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- 
-- Extraindo dados da tabela `video`
-- 


-- --------------------------------------------------------

-- 
-- Estrutura da tabela `video_not`
-- 

CREATE TABLE `video_not` (
  `id` int(3) NOT NULL auto_increment,
  `id_franquia` char(3) NOT NULL default '',
  `id_galeria` varchar(5) NOT NULL default '',
  `categoria` varchar(50) NOT NULL default '',
  `foto` varchar(255) NOT NULL default '',
  `titulo` varchar(255) NOT NULL default '',
  `texto` varchar(255) NOT NULL default '',
  `previa` varchar(255) NOT NULL default '',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- 
-- Extraindo dados da tabela `video_not`
-- 


-- --------------------------------------------------------

-- 
-- Estrutura da tabela `votes`
-- 

CREATE TABLE `votes` (
  `id` int(11) NOT NULL auto_increment,
  `option_id` int(11) NOT NULL,
  `voted_on` datetime NOT NULL,
  `ip` varchar(16) default NULL,
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 AUTO_INCREMENT=6 ;

