CREATE TABLE `guia_registros` (
  `id` int(9) NOT NULL auto_increment,
  `id_cat` varchar(6) NOT NULL default '',
  `id_subcat` varchar(6) NOT NULL default '',
  `nome` varchar(255) NOT NULL default '',
  `endereco` varchar(255) NOT NULL default '',
  `bairro` varchar(255) NOT NULL default '',
  `cidade` varchar(255) NOT NULL default '',
  `estado` varchar(255) NOT NULL default '',
  `cep` varchar(9) NOT NULL default '',
  `fone` varchar(255) NOT NULL default '',
  `site` varchar(255) NOT NULL default '',
  `email` varchar(255) NOT NULL default '',
  `descricao` varchar(255) NOT NULL default '',
  `status` enum('S','N') NOT NULL default 'N',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

